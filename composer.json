{
    "name": "offbeatwp/offbeatwp",
    "type": "wordpress-theme",
    "config": {
        "secure-http": false
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "ssh://git@github.com:offbeatwp/framework.git"
        }
    ],
    "require": {
        "offbeatwp/framework": "master-dev"
    },
    "autoload": {
        "psr-4": {
            "OffbeatWP\\": "app",
            "Modules\\": "modules",
            "Components\\": "components"
        }
    }
}
