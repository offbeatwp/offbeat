{
    "name": "offbeatwp/offbeatwp",
    "type": "wordpress-theme",
    "config": {
        "secure-http": false
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/offbeatwp/framework"
        }
    ],
    "require": {
        "offbeatwp/framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "OffbeatWP\\": "app",
            "Modules\\": "modules",
            "Components\\": "components"
        }
    }
}
